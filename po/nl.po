#, c-format
msgid "Login as %s"
msgstr "Aanmelden als %s"

#: ../src/user-list.vala:830
msgid "Retry"
msgstr "Opnieuw proberen"

#: ../src/user-list.vala:831
#, c-format
msgid "Retry as %s"
msgstr "Opnieuw proberen als %s"

