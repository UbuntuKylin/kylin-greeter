#: ../src/user-list.vala:825
msgid "Log In"
msgstr "Пријави ме"

#: ../src/user-list.vala:826
#, c-format
msgid "Login as %s"
msgstr "Пријави ме као %s"

#: ../src/user-list.vala:830
msgid "Retry"
msgstr "Покушај поново"

#: ../src/user-list.vala:831
#, c-format
